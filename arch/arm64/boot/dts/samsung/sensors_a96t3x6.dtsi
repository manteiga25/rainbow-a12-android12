#if 1

&sw_i2c0 {
	gpios = <SEC_GPIO_REF(AP,pio,42) 0 /* sda */
			SEC_GPIO_REF(AP,pio,41) 0>; /* scl */
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";
	clock-frequency = <100000>;
	a96t3x6: a96t3x6@20 {
		compatible = "a96t3x6";
		reg = <0x20>;
		
		pinctrl-names = "default";
