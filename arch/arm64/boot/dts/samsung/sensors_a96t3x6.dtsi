		a96t3x6,irq_gpio = <SEC_GPIO_REF(AP,pio,79) 0>;
		a96t3x6,ldo_en = <SEC_GPIO_REF(AP,pio,151) 0>;
		a96t3x6,fw_path = "abov/a96t356_a12_main.bin";
		a96t3x6,firmup_cmd = <0x3b>;
	};
};

&pio {
	grip_int: grip-int {
		pins_cmd_dat {
			pinmux = <MTK_PINMUX(SEC_GPIO_NUM(AP,pio,79), 0)>;
			slew-rate = <0>;
			bias-disable;
		};
	};
};

